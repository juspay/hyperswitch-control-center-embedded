{
    "name": "@juspay-tech/hyper-dashboard-embed-react",
    "version": "1.0.1",
    "description": "Hyperswitch Dashboard Embed - React components for embedded SDK",
    "main": "./dist/index.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "rollup -c",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run clean && pnpm run build"
    },
    "keywords": [
        "hyperswitch",
        "dashboard",
        "embedded",
        "react",
        "sdk",
        "embed"
    ],
    "author": "Juspay Technologies",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/kanikabansal08/embed-sdk.git",
        "directory": "react"
    },
    "bugs": {
        "url": "https://github.com/kanikabansal08/embed-sdk/issues"
    },
    "homepage": "https://github.com/kanikabansal08/embed-sdk#readme",
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "react": ">=18 <20",
        "react-dom": ">=18 <20"
    },
    "dependencies": {
        "@juspay-tech/hyper-dashboard-embed-core": "workspace:*"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-replace": "^6.0.3",
        "@types/node": "^24.10.0",
        "@types/react": "^18.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^2.79.2",
        "@rollup/plugin-terser": "^0.4.4",
        "rollup-plugin-typescript2": "^0.36.0",
        "typescript": "^5.9.3"
    }
}